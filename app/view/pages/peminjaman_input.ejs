    <script>
      $('#tambah_pinjam').val(new Date().toISOString().substring(0, 10))

      $('.select2bs4').select2({
        theme: 'bootstrap4'
      })
      user = '<%= session["nama"] %>'

      if (user == "") window.location = "/login"

      document.getElementById("user").innerHTML = `<a href="#" class="d-block">${user} </a>`

      daftar_pinjam()

      kosong_id_pinjam()

      function kosong_id_pinjam() {
        window.id_pinjam = 0
      }

      
      function konfigurasi() {
        $.ajax({
          url: '/konfigurasi1',
          method: "get",
          dataType: "JSON",
          data: {
            tg_pinjam : document.getElementById("tambah_pinjam").value,
          },
          success: function (hasil) {

           
            // document.getElementById("tambah_lama").value = hasil.data[0].lama_pinjam
            document.getElementById("tambah_lama").disabled = hasil.data[0].cek
            $('#tambah_lama').val( hasil.data[0].lama_pinjam).trigger('change') 
           
          },

          error: function (hasil) {
            toastr.error(hasil.responseText)

          }
        })
      }
      pilih_anggota()

      function pilih_anggota() {

        $.ajax({
          url: '/daftar_anggota',
          method: "GET",
          dataType: "json",
          data: {},
          success: function (hasil) {
            vhtml = ""
            for (data of hasil) {
              vhtml += ` <option value = "${data.n_anggota}">${data.nama}</option>`
            }

            $("#edit_nopin").html(vhtml)
            $("#tambah_nopin").html(vhtml)
          },
          error: function (hasil) {
            toastr.error(hasil.responseText)
          }
        })


      }

      pilih_buku()

      function pilih_buku() {

        $.ajax({
          url: '/daftar_buku',
          method: "get",
          dataType: "json",
          data: {},
          success: function (hasil) {
            vhtml = ` <option value = "0">pilih buku</option>`

            for (data of hasil) {
              vhtml += ` <option value = "${data.n_buku}">${data.judul}</option>`
            }

            $("#tambah_judul").html(vhtml)
          },
          error: function (hasil) {
            toastr.error(hasil.responseText)
          }
        })

      }

      function pilih_buku2() {

        $.ajax({
          url: '/daftar_buku',
          method: "post",
          dataType: "json",
          data: {
            'n_penerbit': document.getElementById("tambah_penerbit").value,
          },
          success: function (hasil) {
            vhtml = ""
            if (hasil.length > 1) vhtml = ` <option value = "0">pilih buku</option>`
            for (data of hasil) {
              vhtml += ` <option value = "${data.n_buku}">${data.judul}</option>`

            }

            $("#tambah_judul").html(vhtml)
          },
          error: function (hasil) {
            toastr.error(hasil.responseText)
          }
        })

      }
      pilih_penerbit()

      function pilih_penerbit() {

        vhtml = ``

        $.ajax({
          url: '/daftar_penerbit',
          method: "GET",
          dataType: "json",
          data: {},
          success: function (hasil) {
            vhtml = `<option value = "0">pilih penerbit</option>`
            for (data of hasil) {
              vhtml += ` <option value = "${data.n_penerbit}">${data.nama}</option>`

            }

            $("#tambah_penerbit").html(vhtml)
          },
          error: function (hasil) {
            toastr.error(hasil.responseText)
          }
        })

      }

      function pilih_penerbit2() {

        $.ajax({
          url: '/daftar_penerbit',
          method: "post",
          dataType: "json",
          data: {
            'n_bukum': document.getElementById("tambah_judul").value,
          },
          success: function (hasil) {
            vhtml = ""

            if (hasil.length > 1) vhtml = ` <option value = "0">pilih penerbit</option>`
            for (data of hasil) {
              vhtml += ` <option value = "${data.n_penerbit}">${data.nama}</option>`
            }


            $("#tambah_penerbit").html(vhtml)
          },
          error: function (hasil) {
            toastr.error(hasil.responseText)
          }
        })

      }

      function kosong_n_pemasukan() {
        window.n_pemasukan = 0

      }
      //  batasssssssss

      function daftar_pinjam() {
        $.ajax({
          url: '/daftar_pinjam',
          method: "GET",
          dataType: "json",
          data: {},
          success: function (hasil) {
            vdata = []

            for (kolom of hasil) {
              isi_table = []
              isi_table.push(kolom.no_pinjam, kolom.nama, kolom.judul, kolom.penerbit, kolom.tg_pinjam, kolom
                .lama, kolom.jumlah)
              vdata.push(isi_table)
            }
            // console.log(hasil)
            new gridjs.Grid({
              columns: ["Nomor Pinjam", "Nama Peminjam", "Judul Buku", "Penerbit", "Tanggal Pinjam",
                "Lama Pinjam(Hari)", "Jumlah"
              ],
              data: vdata,
              pagination: {
                enabled: true,
                limit: 5
              },
              search: true,
              language: {
                search: {
                  placeholder: "Cari"
                },
                'pagination': {
                  'previous': 'Sebelumnya',
                  'next': 'Selanjutnya',
                  'showing': 'Menampilkan',
                  'results': 'Hasil',
                  'to': 'Sampai',
                  'of': 'Dari'


                }
              }
            }).render(document.getElementById("table_pinjam"));
          },
          error: function (hasil) {
            toastr.error(hasil.responseText)
          }
        })
      }

      function simpan_pinjam(no_pinjam, kondisi) {
        $.ajax({
          url: '/simpan_pinjam',
          method: "POST",
          dataType: "json",
          data: {
            no_anggota: document.getElementById(`${kondisi}_nopin`).value,
            n_bukum: document.getElementById(`${kondisi}_judul`).value,
            tg_pinjam: document.getElementById(`${kondisi}_pinjam`).value,
            lama: document.getElementById(`${kondisi}_lama`).value,
