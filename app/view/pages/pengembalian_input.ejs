          }

          new gridjs.Grid({
            columns: ["Nomor Pinjam", "Nama Peminjam","Judul Buku","Penerbit","Tanggal Kembali", "Jumlah"],
            data: vdata,
            pagination: {
              enabled: true,
              limit: 5
            },
            search: true,
            language: {
              search: {
                placeholder: "Cari"
              },
              'pagination': {
                'previous': 'Sebelumnya',
                'next': 'Selanjutnya',
                'showing': 'Menampilkan',
                'results': 'Hasil',
                'to': 'Sampai',
                'of': 'Dari'


              }
            }
          }).render(document.getElementById("table_kembali"));


